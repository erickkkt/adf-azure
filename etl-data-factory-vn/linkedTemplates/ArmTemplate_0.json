					"transformations": [
						{
							"name": "FilterIndividual"
						},
						{
							"name": "SelectColumns"
						}
					],
					"scriptLines": [
						"source(output(",
						"          Column_1 as string,",
						"          Column_2 as string,",
						"          Column_3 as string,",
						"          Column_4 as string,",
						"          Column_5 as string,",
						"          Column_6 as string,",
						"          Column_7 as string,",
						"          Column_8 as string,",
						"          Column_9 as string,",
						"          Column_10 as string,",
						"          Column_11 as string,",
						"          Column_12 as string,",
						"          Column_13 as string,",
						"          Column_14 as string,",
						"          Column_15 as string,",
						"          Column_16 as string,",
						"          Column_17 as string,",
						"          Column_18 as string,",
						"          Column_19 as string,",
						"          Column_20 as string,",
						"          Column_21 as string,",
						"          Column_22 as string,",
						"          Column_23 as string,",
						"          Column_24 as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> source1",
						"source1 filter(Column_1=='individual') ~> FilterIndividual",
						"FilterIndividual select(mapColumn(",
						"          InsuredType = Column_1,",
						"          InsuredFirstName = Column_2,",
						"          InsuredSurname = Column_3,",
						"          InsuredBirthday = Column_4,",
						"          Postcode = Column_5,",
						"          Address1 = Column_6,",
						"          Address2 = Column_7,",
						"          Address3 = Column_8,",
						"          Telephone = Column_9,",
						"          EmailAddress = Column_10,",
						"          NumberVehicle = Column_14,",
						"          VehicleRegistration = Column_15,",
						"          InceptionDate = Column_19,",
						"          GrossPremium = Column_20,",
						"          IPT = Column_21,",
						"          NetPremium = Column_22",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> SelectColumns",
						"SelectColumns sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
					]
				}
			},
			"dependsOn": [
