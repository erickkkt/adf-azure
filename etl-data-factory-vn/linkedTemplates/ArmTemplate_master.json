{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "etl-data-factory-vn"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorageDestination_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageDestination'"
		},
		"AzureBlobStorageSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageSource'"
		},
		"AzureBlobStorageTaiHoangSample_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureBlobStorageTaiHoangSample'"
		},
		"AzureBlobStorage_dftaihoang_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_dftaihoang'"
		},
		"AzureSQLDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDb'"
		},
		"AzureBlobStorageTaiHoangSample_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dfstoragevntaihoang.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "etl-data-factory-vn_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorageDestination_connectionString": {
						"value": "[parameters('AzureBlobStorageDestination_connectionString')]"
					},
					"AzureBlobStorageSource_connectionString": {
						"value": "[parameters('AzureBlobStorageSource_connectionString')]"
					},
					"AzureBlobStorageTaiHoangSample_accountKey": {
						"value": "[parameters('AzureBlobStorageTaiHoangSample_accountKey')]"
					},
					"AzureBlobStorage_dftaihoang_connectionString": {
						"value": "[parameters('AzureBlobStorage_dftaihoang_connectionString')]"
					},
					"AzureSQLDb_connectionString": {
						"value": "[parameters('AzureSQLDb_connectionString')]"
					},
					"AzureBlobStorageTaiHoangSample_properties_typeProperties_url": {
						"value": "[parameters('AzureBlobStorageTaiHoangSample_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}